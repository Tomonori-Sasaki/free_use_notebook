<%= form_with(model: youtube, local: true) do |form| %>
  <% if youtube.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(youtube.errors.count, "error") %> prohibited this youtube from being saved:</h2>

      <ul>
      <% youtube.errors.full_messages.each do |message| %>
